<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">


	<bean id="triangle" class="org.sbelei.springdemo.Triangle" autowire="byName">
	</bean>

	<bean id="pointA" class="org.sbelei.springdemo.Point">
		<property name="x" value="0"/>
		<property name="y" value="0"/>
	</bean>

	<bean id="pointB" class="org.sbelei.springdemo.Point">
		<property name="x" value="20"/>
		<property name="y" value="0"/>
	</bean>

	<bean id="pointC" class="org.sbelei.springdemo.Point">
		<property name="x" value="0"/>
		<property name="y" value="20"/>
	</bean>

	<alias name="triangle" alias="ololo"/>
</beans>
